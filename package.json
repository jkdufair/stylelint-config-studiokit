{
	"name": "stylelint-config-studiokit",
	"version": "0.2.0",
	"description": "Shared config for stylelint",
	"keywords": [
		"stylelint",
		"stylelint-config",
		"standard"
	],
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/purdue-tlt/stylelint-config-studiokit.git"
	},
	"main": "index.js",
	"files": [
		"index.js"
	],
	"bugs": {
		"url": "https://github.com/purdue-tlt/stylelint-config-studiokit/issues"
	},
	"homepage": "https://github.com/purdue-tlt/stylelint-config-studiokit#readme",
	"dependencies": {
		"postcss": "^7.0.2",
		"postcss-syntax": "^0.34.0",
		"stylelint-config-standard": "^18.2.0"
	},
	"peerDependencies": {
		"stylelint": "^9.5.0"
	},
	"devDependencies": {
		"babel-eslint": "7.2.3",
		"eslint": "4.10.0",
		"eslint-config-prettier": "^3.0.1",
		"eslint-config-studiokit": "^0.1.0",
		"eslint-config-stylelint": "^8.2.0",
		"eslint-plugin-prettier": "^2.6.2",
		"jest": "^23.6.0",
		"npm-run-all": "^4.1.3",
		"prettier": "1.14.2",
		"remark-cli": "^5.0.0",
		"remark-preset-lint-consistent": "^2.0.2",
		"remark-preset-lint-recommended": "^3.0.2",
		"stylelint": "^9.5.0"
	},
	"scripts": {
		"lint:js": "eslint . --ignore-path .gitignore",
		"lint:md": "remark . --quiet --frail",
		"lint": "run-p lint:*",
		"test": "jest",
		"watch": "jest --watch"
	},
	"eslintConfig": {
		"extends": [
			"stylelint",
			"studiokit"
		]
	},
	"jest": {
		"testEnvironment": "node",
		"verbose": true
	},
	"remarkConfig": {
		"plugins": [
			"preset-lint-recommended",
			"preset-lint-consistent"
		]
	}
}
