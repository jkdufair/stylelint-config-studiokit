{
  "name": "stylelint-config-studiokit",
  "version": "0.1.0",
  "description": "Shared config for stylelint",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "standard"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purdue-tlt/stylelint-config-studiokit.git"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "bugs": {
    "url": "https://github.com/purdue-tlt/stylelint-config-studiokit/issues"
  },
  "homepage": "https://github.com/purdue-tlt/stylelint-config-studiokit#readme",
  "dependencies": {
    "stylelint-config-standard": "^17.0.0"
  },
  "peerDependencies": {
    "stylelint": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-stylelint": "^7.0.0",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "remark-cli": "^4.0.0",
    "remark-preset-lint-consistent": "^2.0.0",
    "remark-preset-lint-recommended": "^3.0.0",
    "stylelint": "^8.0.0"
  },
  "scripts": {
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail",
    "lint": "run-p lint:*",
    "test": "jest",
    "watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "verbose": true
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      "preset-lint-consistent"
    ]
  }  
}
